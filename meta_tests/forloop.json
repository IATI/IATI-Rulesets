{
    "//iati-activity": {
        "loop": {
            "cases": [
                {
                    "foreach": "sector[@vocabulary != '1']/@vocabulary",
                    "do": {
                        "strict_sum": {
                            "cases": [
                                {
                                    "paths": [ "sector[@vocabulary = '$1']/@percentage" ],
                                    "sum": 100
                                }
                            ]
                        }
                    },
                    "subs": ["paths"]
                }
            ]
        }
    }
}
